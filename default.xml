<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <default sync-j="4" revision="pyro"/>

  <remote fetch="https://git.yoctoproject.org/git" name="yocto"/>
  <remote fetch="https://github.com/openembedded"  name="oe"/>
  <remote fetch="https://github.com/Freescale"     name="freescale"/>
  <remote fetch="git://github.com/meta-qt5"        name="QT5"/>
  <remote fetch="https://github.com/OSSystems"     name="OSSystems"/>
  <remote fetch="https://github.com/sbabic"        name="sbabic"/>
  <remote fetch="https://github.com/varigit"       name="variscite"/>

  <project remote="yocto"     upstream="pyro" name="poky" path="sources/poky"/>
  <project remote="yocto"     upstream="pyro" name="meta-freescale" path="sources/meta-freescale"/>
  <project remote="oe"        upstream="pyro" name="meta-openembedded" path="sources/meta-openembedded"/>
  <project remote="freescale" upstream="pyro" name="meta-freescale-3rdparty" path="sources/meta-freescale-3rdparty"/>
  <project remote="freescale" upstream="pyro" name="meta-freescale-distro" path="sources/meta-freescale-distro"/>
  <project remote="QT5"       upstream="pyro" name="meta-qt5" path="sources/meta-qt5"/>
  <project remote="OSSystems" upstream="morty" name="meta-browser" path="sources/meta-browser"/>
  <project remote="sbabic"    upstream="pyro" name="meta-swupdate" path="sources/meta-swupdate"/>
  <project remote="variscite"  upstream="pyro" name="meta-variscite-fslc" path="sources/meta-variscite-fslc"/>

  <project remote="variscite" revision="pyro" name="variscite-bsp-base" path="sources/base">
	<copyfile dest="README" src="README"/>
	<copyfile dest="setup-environment" src="setup-environment"/>
  </project>

</manifest>
